<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Equipment">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/addmoreequip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:text="@string/add_another_vehicle"
            android:layout_margin="5dp"
            android:background="@color/cyan_100"
            android:textStyle="bold"
            android:textColor="@color/lime_900"
            android:padding="8dp"
            android:onClick="addmoreforsupervisor"/>
        <Button
            android:id="@+id/equipinfosend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:text="@string/sendtoAdmin"
            android:layout_margin="5dp"
            android:background="@color/cyan_100"
            android:textStyle="bold"
            android:textColor="@color/lime_900"
            android:padding="8dp"
            android:onClick="sendtoAdmin"/>

    </RelativeLayout>


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rl">

    </RelativeLayout>



</LinearLayout>

